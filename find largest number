#include<stdio.h>

int returnmax(int arr[] ,int n){
    int max =0;
    for(int i =0; i<n; i++){
        if (arr[i]>max)
        {
            max = arr[i];
        }
    }
    return max;
}



int main(){
      int arr[] = {1,2,3,4,56,7,2,2,5,3,4};
      int max = returnmax(arr ,11);
      printf("The max element in the array is : %d\n" , max);
      return 0;
}
